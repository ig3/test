'use strict';

const t = require('../..');

t.test('async test', t => {
  t.pass('immediate assertion');
  setTimeout(
    () => {
      t.pass('deferred assertion');
      t.end();
    },
    0
  );
});
/*
TAP version 13
# async test
ok 1 immediate assertion
ok 2 deferred assertion
ok 3 async test

1..3
# tests 3
# pass  3

# ok
*/
