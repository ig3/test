'use strict';

const t = require('../..');

t.skip('skip one', t => {
  t.pass('skip one pass');
  t.end();
});
t.skip('skip two', t => {
  t.pass('skip two pass');
  t.end();
});
/*
TAP version 13
# skip one
ok 1 test: skip one # SKIP
# skip two
ok 2 test: skip two # SKIP

1..2
# tests 2
# pass  0
# skip  2
*/
